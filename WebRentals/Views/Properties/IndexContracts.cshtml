@model IEnumerable<Web_ManagementHouseRentals.Data.Entities.Contract>

@{
    ViewData["Title"] = "IndexContracts";
}

<h1>Contracts</h1>

<!--/ Intro Single star /-->
<section class="intro-single">
    <div class="container">
        <div class="row">
            <div class="col-md-12 col-lg-8">
                <div class="title-single-box">
                    <h1 class="title-single">My contracts</h1>
                    <span class="color-text-a"></span>
                </div>
            </div>
        </div>
    </div>
</section>
<!--/ Intro Single End /-->
<div class="container">
    <table class="table" id="indexPropertyCustomerTable">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Property.Owner)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Tenant)
                </th>
                <th>
                    Property @Html.DisplayNameFor(model => model.Property.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Start)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Property.Owner.FullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Tenant.FullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Property.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Start)
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>